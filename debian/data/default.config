server {
    listen 8002;
    listen 8004;
    listen 8005;

	server_name localhost;
    host 127.0.0.1;
    root docs/fusion_web/;
    # client_max_body_size 3000000;
	index index.html abc.html;
    error_page 404 error_pages/404.html;

    location / {
        allow_methods  DELETE POST GET;
        autoindex off;
    }

    location /tours {
        autoindex on;
        index tours1.html;
        allow_methods GET POST;
    }

	location /red {
		return 303 /tours;
	}

    location /cgi {
        root ./;
        allow_methods POST DELETE;
        index index.php;
        cgi_path /opt/homebrew/bin/php-cgi;
        cgi_ext .php;
    }
}

server {
    listen 8003;
    host 127.0.0.1;
    root docs/fusion_web/;
	index siege.html;
}


